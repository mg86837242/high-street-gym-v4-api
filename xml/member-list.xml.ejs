<!DOCTYPE memberList [ <!ELEMENT memberList (member*)>
<!ELEMENT member (firstName, lastName, phone, email, age, gender, address)>
<!ELEMENT firstName (#PCDATA)>
<!ELEMENT lastName (#PCDATA)>
<!ELEMENT phone (#PCDATA)>
<!ELEMENT email (#PCDATA)>
<!ELEMENT age (#PCDATA)>
<!ELEMENT gender (#PCDATA)>
<!ELEMENT address (lineOne, lineTwo, suburb, postcode, state, country)>
<!ELEMENT lineOne (#PCDATA)>
<!ELEMENT lineTwo (#PCDATA)>
<!ELEMENT suburb (#PCDATA)>
<!ELEMENT postcode (#PCDATA)>
<!ELEMENT state (#PCDATA)>
<!ELEMENT country (#PCDATA)>
]>
<memberList>
  <% for (const member of members) { %>
  <member>
    <firstName><%- member.firstName %></firstName>
    <lastName><%- member.lastName %></lastName>
    <phone><%- member.phone %></phone>
    <email><%- member.email %></email>
    <age><%- member.age %></age>
    <gender><%- member.gender %></gender>
    <address>
      <lineOne><%- member.address.lineOne %></lineOne>
      <lineTwo><%- member.address.lineTwo %></lineTwo>
      <suburb><%- member.address.suburb %></suburb>
      <postcode><%- member.address.postcode %></postcode>
      <state><%- member.address.state %></state>
      <country><%- member.address.country %></country>
    </address>
  </member>
  <% } %>
</memberList>
